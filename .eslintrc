{
	"parser": "babel-eslint",
	"rules": {
		"max-len": [1, 120, 2, {"ignoreComments": true}],
		"react/jsx-uses-vars": 1,
		"react/react-in-jsx-scope": 1,
		"globals": { "fetch": true },
		"no-console": ["error", { "allow": ["warn", "error"] }]
	},
	"extends": ["airbnb-base", "eslint:recommended", "plugin:react/recommended"]

}
